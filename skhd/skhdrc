cmd - r : ~/.config/skhd/scripts/reload.sh

# Switch spaces
cmd - 1 : yabai -m space --focus 1
cmd - 2 : yabai -m space --focus 2
cmd - 3 : yabai -m space --focus 3
cmd - 4 : yabai -m space --focus 4
cmd - 5 : yabai -m space --focus 5
cmd - 6 : yabai -m space --focus 6
cmd - 7 : yabai -m space --focus 7
cmd - 8 : yabai -m space --focus 8
cmd - 9 : yabai -m space --focus 9
cmd - 0 : yabai -m space --focus 10

# Move window to space
cmd + shift - 1 : ~/.config/skhd/scripts/window-to-space.sh 1
cmd + shift - 2 : ~/.config/skhd/scripts/window-to-space.sh 2
cmd + shift - 3 : ~/.config/skhd/scripts/window-to-space.sh 3
cmd + shift - 4 : ~/.config/skhd/scripts/window-to-space.sh 4
cmd + shift - 5 : ~/.config/skhd/scripts/window-to-space.sh 5
cmd + shift - 6 : ~/.config/skhd/scripts/window-to-space.sh 6
cmd + shift - 7 : ~/.config/skhd/scripts/window-to-space.sh 7
cmd + shift - 8 : ~/.config/skhd/scripts/window-to-space.sh 8
cmd + shift - 9 : ~/.config/skhd/scripts/window-to-space.sh 9
cmd + shift - 0 : ~/.config/skhd/scripts/window-to-space.sh 10

# Move focus
cmd - h : yabai -m window --focus west
cmd - j : yabai -m window --focus south
cmd - k : yabai -m window --focus north
cmd - l : yabai -m window --focus east

# Custom openers
cmd - return : open -na "kitty"
